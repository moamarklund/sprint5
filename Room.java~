public class Room{
    private String name;
    private boolean doorOpen;
    private boolean north;
    private boolean east;
    private boolean west;
    private boolean south;

    public Room(){
	this.name = name;
	this.doorOpen = false;
	this.doorDirection = false;
    }
	     
    public void createDoorNorth(){
	north = false;
	east = false;
	west = false;
	south = true;
	doorOpen = true; 
	
    }
}
